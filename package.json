{
    "name": "danganronpa-modding-tools",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "dependencies": {
        "unzipper": "^0.12.3"
    },
    "scripts": {
        "start": "pnpm run build && pnpm run game",
        "compile": "cd lin-compiler/lin_compiler && dotnet build -c Release",
        "ext": "pnpm --filter lindecompilerhelper install-extension",
        "reset": "pnpm compile && node scripts/src/extract-linscript.js",
        "unpack": "node scripts/src/unpack-base-files.js",
        "extract-recursive": "node scripts/src/extract-recursive.js",
        "setup": "node scripts/src/zip-game-files.js && pnpm run unpack && pnpm run reset",
        "select": "node scripts/src/select.js",
        "verify": "node scripts/src/verify.js",
        "build": "node scripts/src/build.js",
        "clear-proton": "node scripts/src/clear-proton.js",
        "copy": "node scripts/src/generator.js",
        "game": "node scripts/src/launch-game.js",
        "investigate": "node scripts/src/investigate.js",
        "validate-paks": "node scripts/src/validate-paks.js"
    },
    "packageManager": "pnpm@10.22.0+sha512.bf049efe995b28f527fd2b41ae0474ce29186f7edcb3bf545087bd61fbbebb2bf75362d1307fda09c2d288e1e499787ac12d4fcb617a974718a6051f2eee741c"
}